{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Testing": {
    "DisableRateLimit": true
  },
  "VoiceService": {
    "FoundryEndpoint": "https://test-foundry.mock/api/projects/test-project",
    "FoundryVoiceEndpoint": "wss://test-foundry.mock/voice",
    "SessionTimeoutMinutes": 5,
    "MaxConcurrentSessions": 3
  },
  "Mcp": {
    "EnableSse": true,
    "EnableWebSocket": false,
    "ConversationLifetimeHours": 1,
    "EnableDetailedLogging": false,
    "MaxQueryHistorySize": 10,
    "SseHeartbeatIntervalSeconds": 60
  }
}
