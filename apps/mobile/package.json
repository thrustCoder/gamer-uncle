{
  "name": "gamer-uncle",
  "version": "1.3.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ci": "playwright test --reporter=junit --output-dir=test-results",
    "test:e2e:dev": "E2E_BASE_URL=https://gamer-uncle-dev-mobile.azurewebsites.net playwright test",
    "test:install": "playwright install --with-deps",
    "test:setup": "./setup-e2e.sh && npm run test:e2e",
    "test:all": "npm test && npm run test:e2e"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-masked-view/masked-view": "github:react-native-masked-view/masked-view",
    "@react-native-picker/picker": "2.11.0",
    "@react-navigation/native": "^7.1.10",
    "@react-navigation/stack": "^7.3.3",
    "axios": "^1.9.0",
    "expo": "~53.0.11",
    "expo-av": "~15.1.6",
    "expo-constants": "~17.1.6",
    "expo-status-bar": "~2.2.3",
    "lottie-react-native": "^7.2.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.3",
    "react-native-confetti-cannon": "^1.5.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@playwright/test": "^1.48.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.10",
    "@types/react-native": "^0.73.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.4",
    "jest-expo": "^53.0.9",
    "react-test-renderer": "^19.0.0",
    "ts-jest": "^29.1.2",
    "typescript": "~5.8.3"
